<?xml version="1.0"?>
<launch>
<include file="$(find bullet_reasoning_demo)/launch/bullet_reasoning.launch"/>
<group ns="quad1">
<param name="robot_description" command="$(find xacro)/xacro.py '$(find hector_quadrotor_description)/urdf/quadrotor.gazebo.xacro'"/>
</group>


<group ns="genius">
<arg name="model" default="$(find sherpa_genius_description)/urdf/sherpa_busy_genius.xacro"/>
   <!-- send the robot XML to param server -->
   <param name="robot_description" command="$(find xacro)/xacro.py '$(arg model)'" />
  <!-- We resume the logic in empty_world.launch, changing only the name of the world to be launched -->
  <arg name="x" default="0"/>
  <arg name="y" default="0"/>
  <arg name="z" default="0"/>
  <arg name="R" default="0"/>
  <arg name="P" default="0"/>
  <arg name="Y" default="0"/>
  <arg name="robot_name" default="busy_genius"/>
  <arg name="robot_namespace" default="busy_genius"/> 
  
  
  <node name="spawn_busy_genius" pkg="gazebo_ros" type="spawn_model" args="-param robot_description -urdf -x $(arg x) -y $(arg y) -z $(arg z) -R $(arg R) -P $(arg P) -Y $(arg Y)  -model $(arg robot_name) -robot_namespace $(arg robot_namespace)"/> 
<!--/group--><!--node name="spawn_rmax" pkg="gazebo_ros" type="spawn_model" args="-file $(find sherpa_rmax_description)/urdf/sherpa_rmax.urdf -urdf -z 1 -model Sherpa_Rmax"/--> 
   <node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher" output="screen" >
     <param name="publish_frequency" type="double" value="50.0" />
   <param name="tf_prefix" type="string" value="$(arg robot_namespace)" />
   </node>


<!--<param name="robot_description" command="$(find xacro)/xacro.py $(find sherpa_genius_description)/urdf/sherpa_busy_genius.xacro busy_genius -robot_namespace" />-->
<!--<param name="robot_description" command="$(find sherpa_genius_description)/urdf/sherpa_busy_genius.urdf" />-->

</group>

</launch>